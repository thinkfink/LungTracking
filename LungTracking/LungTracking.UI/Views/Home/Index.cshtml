}

<!-- Bootstrap core CSS -->
<link href="vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

<!-- Custom styles for this template -->
<link href="business-frontpage.css" rel="stylesheet">



<!-- Header -->
<header class="py-5 mb-5" style="margin-top:-55px;background-color:white">
    <div class="container h-32">
        <div class="row h-100 align-items-center">
            <div class="col-lg-12">
                <h1 class="display-4 mt-5 mb-2" style="float:left;color:#03045e">Lung Tracking</h1>
            </div>
        </div>
    </div>
</header>

